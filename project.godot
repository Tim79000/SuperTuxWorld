config_version=5

[application]

config/name="SuperTuxWorld"
config/description="An open-source 3D platformer for Windows and Mac."
run/main_scene="res://scenes/level/movement_test.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://images/splash.png"
config/icon="res://icon.svg"
boot_splash/minimum_display_time=9

[autoload]

MenuTemplateManager="*res://addons/EasyMenus/Nodes/menu_template_manager.tscn"
ControllerIcons="*res://addons/controller_icons/ControllerIcons.gd"

[debug]

settings/profiler/max_functions=8192

[display]

window/size/viewport_width=1008
window/size/mode=4
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/qodot/plugin.cfg")

